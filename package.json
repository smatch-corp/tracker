{
  "name": "@smatch-corp/untracker",
  "version": "0.1.2",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "core/index.cjs",
  "module": "core/index.mjs",
  "types": "core/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "core",
    "react",
    "providers"
  ],
  "scripts": {
    "build": "vite build -c vite.config.mjs && vite build -c vite.config.react.mjs && vite build -c vite.config.providers.mjs && tsc",
    "prepack": "yarn build"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./core/index.d.ts",
        "default": "./core/index.mjs"
      },
      "require": {
        "types": "./core/index.d.ts",
        "default": "./core/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./react/index.d.ts",
        "default": "./react/index.mjs"
      },
      "require": {
        "types": "./react/index.d.ts",
        "default": "./react/index.cjs"
      }
    },
    "./providers/*": {
      "import": {
        "types": "./providers/*.d.ts",
        "default": "./providers/*.mjs"
      },
      "require": {
        "types": "./providers/*.d.ts",
        "default": "./providers/*.cjs"
      }
    }
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@hackler/react-sdk": "^11.11.0",
    "@types/mixpanel-browser": "^2.38.1",
    "@types/react": "^18.0.27",
    "@vitest/ui": "^0.28.4",
    "globby": "^13.1.3",
    "happy-dom": "^8.2.6",
    "mixpanel-browser": "^2.45.0",
    "react": "^18.2.0",
    "typescript": "^4.9.5",
    "unstorage": "^1.4.0",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.28.4",
    "vitest-mock-extended": "^1.0.9"
  },
  "peerDependencies": {
    "@hackler/react-sdk": "*",
    "mixpanel-browser": "*",
    "react": "^17 || ^18",
    "unstorage": "^1"
  },
  "peerDependenciesMeta": {
    "@hackler/react-sdk": {
      "optional": true
    },
    "mixpanel-browser": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "ts-essentials": "^9.3.1"
  }
}
