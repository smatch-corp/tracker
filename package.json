{
  "name": "@smatch-corp/tracker",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/core/index.cjs",
  "module": "dist/core/index.mjs",
  "types": "dist/core/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/core/index.d.ts",
        "default": "./dist/core/index.mjs"
      },
      "require": {
        "types": "./dist/core/index.d.ts",
        "default": "./dist/core/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/react/index.mjs"
      },
      "require": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/react/index.cjs"
      }
    },
    "./providers/ga4": {
      "import": {
        "types": "./dist/providers/ga4/index.d.ts",
        "default": "./dist/providers/ga4/index.mjs"
      },
      "require": {
        "types": "./dist/providers/ga4/index.d.ts",
        "default": "./dist/providers/ga4/index.cjs"
      }
    }
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@vitest/ui": "^0.28.4",
    "happy-dom": "^8.2.6",
    "react": "^18.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "vitest-mock-extended": "^1.0.9"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
